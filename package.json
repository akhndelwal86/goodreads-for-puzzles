{
  "name": "goodreads-for-puzzles",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:clean": "rm -rf .next && next dev",
    "dev:deep": "rm -rf .next .swc node_modules/.cache && npm cache clean --force && next dev",
    "dev:fresh": "rm -rf .next .swc node_modules && npm cache clean --force && npm install && next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "cache:clear": "rm -rf .next .swc node_modules/.cache && npm cache clean --force",
    "cache:reset": "rm -rf .next .swc node_modules && npm cache clean --force && npm install",
    "fix:permissions": "chflags -R nouchg node_modules 2>/dev/null || true",
    "fresh": "npm run fix:permissions && npm run cache:reset && npm run dev",
    "debug:cache": "echo 'Cache directories:' && ls -la .next .swc node_modules/.cache 2>/dev/null || echo 'Cache directories not found'",
    "db:schema": "curl -s http://localhost:3000/api/db/schema",
    "db:schema:md": "curl -s 'http://localhost:3000/api/db/schema?format=markdown'",
    "db:migrate:status": "curl -s http://localhost:3000/api/db/migrate",
    "db:migrate:run": "curl -s -X POST http://localhost:3000/api/db/migrate -H 'Content-Type: application/json'",
    "db:sql": "curl -s -X PUT http://localhost:3000/api/db/migrate -H 'Content-Type: application/json'",
    "db:sync-schema": "node scripts/sync-schema.js",
    "db:connect": "./scripts/db-connect.sh"
  },
  "dependencies": {
    "@alloc/quick-lru": "^5.2.0",
    "@clerk/clerk-react": "^5.33.0",
    "@clerk/nextjs": "^6.23.3",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-checkbox": "^1.3.2",
    "@radix-ui/react-collapsible": "^1.1.11",
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-dropdown-menu": "^2.1.15",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-popover": "^1.1.14",
    "@radix-ui/react-scroll-area": "^1.2.9",
    "@radix-ui/react-select": "^2.2.5",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slider": "^1.3.5",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-switch": "^1.2.5",
    "@radix-ui/react-tabs": "^1.1.12",
    "@supabase/storage-js": "^2.8.0",
    "@supabase/supabase-js": "^2.50.3",
    "autoprefixer": "^10.4.21",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "dlv": "^1.1.3",
    "dot-case": "^3.0.4",
    "dotenv": "^17.0.1",
    "lucide-react": "^0.525.0",
    "next": "15.3.4",
    "no-case": "^3.0.4",
    "openai": "^5.8.2",
    "postcss": "^8.5.6",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-remove-scroll": "^2.7.1",
    "svix": "^1.68.0",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^3.4.17",
    "tailwindcss-animate": "^1.0.7",
    "ts-interface-checker": "^1.0.2"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.3.4",
    "tw-animate-css": "^1.3.4",
    "typescript": "^5"
  }
}
